{
  "name": "graphite",
  "version": "0.0.1",
  "type": "module",
  "bin": {
    "graphite": "bin/start.js"
  },
  "main": "./dist/graphite.umd.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/graphite.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/graphite.es.js",
      "require": "./dist/graphite.umd.js"
    },
    "./graphite.css": "./dist/graphite.css"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "vite",
    "build:static": "vite build --mode static",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build:static",
    "deploy": "gh-pages -d gh-pages",
    "build": "tsc -b && vite build",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-tree-view": "^8.10.2",
    "allotment": "^1.20.4",
    "d3": "^7.9.0",
    "d3-graphviz": "^5.6.0",
    "dagre": "^0.8.5",
    "elkjs": "^0.11.0",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "http-server": "^14.1.1",
    "js-yaml": "^4.1.0",
    "jsonc-parser": "^3.3.1",
    "react-router-dom": "^7.8.2",
    "reactflow": "^11.11.4"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/d3": "^7.4.3",
    "@types/d3-graphviz": "^2.6.10",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4"
  }
}
